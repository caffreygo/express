page {
  background-color: #f5f5f5;
}

.container {
  display: flex;
  flex-direction: column;
  margin-top: 20rpx;
}

.item {
  width: 100%;
  height: 98rpx;
  background-color: white;
  border-bottom: 1px solid #ededed;
}

.item-text {
  line-height: 98rpx;
  font-size: 32rpx;
  margin-left: 32rpx;
  vertical-align: middle;
}

.item-image {
  float: right;
  height: 32rpx;
  width: 32rpx;
  margin-top: 33rpx;
  margin-right: 8px;
  vertical-align: middle;
}

.login-out-text {
  font-size: 29rpx;
  color: #FF4D4F;
  margin-left: 286rpx;
  line-height: 98rpx;
}
